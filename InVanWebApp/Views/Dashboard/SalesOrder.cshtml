
@{
    ViewBag.Title = "SalesOrder";
    Layout = "~/Views/Shared/MainMaster.cshtml";
}
<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Dashboard</title>
    <!-- plugins:css -->
    <link rel="stylesheet" href="~/Theme/MainContent/vendors/iconfonts/font-awesome/css/all.min.css">
    <link rel="stylesheet" href="~/Theme/MainContent/vendors/css/vendor.bundle.base.css">
    <link rel="stylesheet" href="~/Theme/MainContent/vendors/css/vendor.bundle.addons.css">
    <!-- endinject -->
    <!-- plugin css for this page -->
    <!-- End plugin css for this page -->
    <!-- inject:css -->
    <link rel="stylesheet" href="~/Theme/MainContent/css/style.css">
    <!-- endinject -->
    <link rel="shortcut icon" href="http://www.urbanui.com/" />
    <style>
        p.dashboardfontsize {
            font-size: larger;
        }
    </style>
    <title>Sales Order</title>
</head>
<body>
    <div class="content-wrapper" runat="server" id="divPanel1">
        <div class="page-header">
            <h4 class="page-title">SALES ORDER</h4>
            <nav aria-label="breadcrum">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item">
                        <i class="fa fa-home"></i><a href="@Url.Action("Index","Dashboard")">Master</a>
                    </li>
                    <li class="breadcrumb-item active">Sales Orders</li>
                </ol>
            </nav>
        </div>
        <hr />
        <div class="panel-body">
            <div class="row">
                <div class="col-md-12">
                    <div class="card">
                        <div class="card-body">
                           
                            <div class="row">
                                <div class="col-md-12 grid-margin stretch-card">
                                    <div class="card">
                                        <div class="card-body">
                                            <h4 class="card-title">
                                                <i class="fas fa-gift"></i>
                                                Sales Orders
                                            </h4>
                                            <canvas id="orders-chart"></canvas>
                                            <div id="orders-chart-legend" class="orders-chart-legend"></div>
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <script src="~/Theme/MainContent/vendors/js/vendor.bundle.base.js"></script>
        <script src="~/Theme/MainContent/vendors/js/vendor.bundle.addons.js"></script>
        <script src="~/Theme/MainContent/js/off-canvas.js"></script>
        <script src="~/Theme/MainContent/js/hoverable-collapse.js"></script>
        <script src="~/Theme/MainContent/js/misc.js"></script>
        <script src="~/Theme/MainContent/js/settings.js"></script>
        <script src="~/Theme/MainContent/js/todolist.js"></script>
        <script src="~/Theme/MainContent/js/dashboard.js"></script>
    </div>
</body>

</html>


