<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Vangi Foods</title>
    <!-- plugins:css -->
    <link href="~/Theme/MainContent/vendors/iconfonts/font-awesome/css/all.min.css" rel="stylesheet" />
    <link href="~/Theme/MainContent/vendors/css/vendor.bundle.addons.css" rel="stylesheet" />
    <link href="~/Theme/MainContent/vendors/css/vendor.bundle.base.css" rel="stylesheet" />
