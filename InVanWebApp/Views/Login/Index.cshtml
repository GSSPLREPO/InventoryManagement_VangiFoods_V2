    <!-- endinject -->
    <link rel="shortcut icon" href="~/Theme/MainContent/images/favicon.png" />
</head>

<body>
    <div class="container-scroller">
        <div class="container-fluid page-body-wrapper full-page-wrapper">
            <div class="content-wrapper d-flex align-items-stretch auth auth-img-bg">
                <div class="row flex-grow">
                    <div class="col-md-5 d-flex align-items-center justify-content-center">
                        <div class="auth-form-transparent text-left p-3">
                            <div class="brand-logo">
                                <img src="~/Theme/MainContent/images/logo.png"  alt="Vangi Foods"/>
                                <h4>Welcome!</h4>
                                <!-- <h6 class="font-weight-light">Happy to see you again!</h6> -->
                            </div>
                            <form class="pt-3">
                                <div class="form-group">
                                    <label for="exampleInputEmail">Username</label>
                                    <div class="input-group">
                                        <div class="input-group-prepend bg-transparent">
                                            <span class="input-group-text bg-transparent border-right-0">
                                                <i class="fa fa-user text-primary"></i>
                                            </span>
                                        </div>
                                        <input type="text" class="form-control form-control-lg border-left-0" id="exampleInputEmail" placeholder="Username">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="exampleInputPassword">Password</label>
                                    <div class="input-group">
                                        <div class="input-group-prepend bg-transparent">
                                            <span class="input-group-text bg-transparent border-right-0">
                                                <i class="fa fa-lock text-primary"></i>
                                            </span>
                                        </div>
                                        <input type="password" class="form-control form-control-lg border-left-0" id="exampleInputPassword" placeholder="Password">
                                    </div>
                                </div>
                                <div class="my-2 d-flex justify-content-between align-items-center">
                                    <div class="form-check">
                                        <label class="form-check-label text-muted">
                                            <input type="checkbox" class="form-check-input">
                                            Keep me signed in
                                        </label>
                                    </div>
                                    <a href="#" class="auth-link text-black">Forgot password?</a>
                                </div>
                                <div class="my-3">
                                    @*<a class="btn btn-block btn-primary btn-lg font-weight-medium auth-form-btn" href="../../index-2.html">LOGIN</a>*@
                                    @*<a class="btn btn-block btn-primary btn-lg font-weight-medium auth-form-btn">LOGIN</a>*@
                                    <a href="@Url.Action("Index","Unit")" class="btn btn-block btn-primary btn-lg font-weight-medium auth-form-btn">Login</a>
                                </div>
                                <!-- <div class="mb-2 d-flex">
                                  <button type="button" class="btn btn-facebook auth-form-btn flex-grow mr-1">
                                    <i class="fab fa-facebook-f mr-2"></i>Facebook
                                  </button>
                                  <button type="button" class="btn btn-google auth-form-btn flex-grow ml-1">
                                    <i class="fab fa-google mr-2"></i>Google
                                  </button>
                                </div>
                                <div class="text-center mt-4 font-weight-light">
                                  Don't have an account? <a href="register-2.html" class="text-primary">Create</a>
                                </div>
                                 -->
                            </form>
                        </div>
                    </div>
                    <div class="col-md-7 offset-1 offset-lg-0 login-half-bg d-flex flex-row">
                        <p class="text-black font-weight-medium text-center flex-grow align-self-end">Copyright &copy; 2021  All rights reserved by Garima System Solutions.</p>
                    </div>
                </div>
            </div>
            <!-- content-wrapper ends -->
        </div>
        <!-- page-body-wrapper ends -->
    </div>
    <!-- container-scroller -->
    <!-- plugins:js -->
    <script src="~/Theme/MainContent/vendors/js/vendor.bundle.addons.js"></script>
    <script src="~/Theme/MainContent/vendors/js/vendor.bundle.base.js"></script>

    <!-- endinject -->
    <!-- inject:js -->
    <script src="~/Theme/MainContent/js/off-canvas.js"></script>
    <script src="~/Theme/MainContent/js/hoverable-collapse.js"></script>
    <script src="~/Theme/MainContent/js/misc.js"></script>
    <script src="~/Theme/MainContent/js/settings.js"></script>
    <script src="~/Theme/MainContent/js/todolist.js"></script>
    <!-- endinject -->
</body>


